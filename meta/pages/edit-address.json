                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Address",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "net5wasmconn.updateAddress",
                        "parameters": [
                          {
                            "name": "Id",
                            "value": "${parameters.Id}"
                          },
                          {
                            "name": "Address",
                            "value": "${address}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${address}",
                            "condition": "result.StatusCode != System.Net.HttpStatusCode.PreconditionFailed"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result.StatusCode == System.Net.HttpStatusCode.PreconditionFailed",
                            "valueType": "bool"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Net5Wasm.Models.Net5Wasmconn.Address",
                  "data": "${address}",
                  "visible": "${address != null && canEdit}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "net5wasmconn.getAddressById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
          {
            "name": "address",
            "type": "set",
            "value": "${result}",
            "valueType": "Net5Wasm.Models.Net5Wasmconn.Address"
          },
          {
            "name": "canEdit",
            "type": "set",
            "value": "result != null",
            "valueType": "bool"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Address",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}