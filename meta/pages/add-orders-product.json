{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Order",
                              "component": "OrderId",
                              "name": "OrderIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getOrdersForOrderIdResult}",
                              "textProperty": "UserId",
                              "valueProperty": "Id",
                              "placeholder": "Choose Order",
                              "valueType": "string",
                              "showSearch": false,
                              "count": "${getOrdersForOrderIdCount}",
                              "type": "dropdown-datagrid",
                              "name": "OrderId",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "events": {
                                "loadData": [
                                  {
                                    "name": "net5wasmconn.getOrders",
                                    "parameters": [
                                      {
                                        "name": "$top",
                                        "value": "${event.top}"
                                      },
                                      {
                                        "name": "$skip",
                                        "value": "${event.skip}"
                                      },
                                      {
                                        "name": "$orderby",
                                        "value": "${event.orderby}"
                                      },
                                      {
                                        "name": "$filter",
                                        "value": "${event.filter}"
                                      },
                                      {
                                        "name": "$count",
                                        "value": "true"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "getOrdersForOrderIdResult",
                                        "type": "set",
                                        "value": "${result.Value.AsODataEnumerable()}",
                                        "valueType": "IEnumerable<Net5Wasm.Models.Net5Wasmconn.Order>"
                                      },
                                      {
                                        "name": "getOrdersForOrderIdCount",
                                        "type": "set",
                                        "value": "result.Count"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "value": "${ordersproduct.OrderId}"
                            },
                            {
                              "type": "required-validator",
                              "name": "OrderIdRequiredValidator",
                              "component": "OrderId",
                              "text": "OrderId is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Product",
                              "component": "ProductId",
                              "name": "ProductIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getProductsForProductIdResult}",
                              "textProperty": "Name",
                              "valueProperty": "Id",
                              "placeholder": "Choose Product",
                              "valueType": "int",
                              "showSearch": false,
                              "count": "${getProductsForProductIdCount}",
                              "type": "dropdown-datagrid",
                              "name": "ProductId",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "events": {
                                "loadData": [
                                  {
                                    "name": "net5wasmconn.getProducts",
                                    "parameters": [
                                      {
                                        "name": "$top",
                                        "value": "${event.top}"
                                      },
                                      {
                                        "name": "$skip",
                                        "value": "${event.skip}"
                                      },
                                      {
                                        "name": "$orderby",
                                        "value": "${event.orderby}"
                                      },
                                      {
                                        "name": "$filter",
                                        "value": "${event.filter}"
                                      },
                                      {
                                        "name": "$count",
                                        "value": "true"
                                      }
                                    ],
                                    "then": [
                                      {
                                        "name": "getProductsForProductIdResult",
                                        "type": "set",
                                        "value": "${result.Value.AsODataEnumerable()}",
                                        "valueType": "IEnumerable<Net5Wasm.Models.Net5Wasmconn.Product>"
                                      },
                                      {
                                        "name": "getProductsForProductIdCount",
                                        "type": "set",
                                        "value": "result.Count"
                                      }
                                    ],
                                    "type": "invoke"
                                  }
                                ]
                              },
                              "value": "${ordersproduct.ProductId}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ProductIdRequiredValidator",
                              "component": "ProductId",
                              "text": "ProductId is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Created On",
                              "component": "CreatedOn",
                              "name": "CreatedOnLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "CreatedOn",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${ordersproduct.CreatedOn}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CreatedOnRequiredValidator",
                              "component": "CreatedOn",
                              "text": "CreatedOn is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Modified On",
                              "component": "ModifiedOn",
                              "name": "ModifiedOnLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "ModifiedOn",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${ordersproduct.ModifiedOn}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Quantity",
                              "component": "Quantity",
                              "name": "QuantityLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "Quantity",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${ordersproduct.Quantity}"
                            },
                            {
                              "type": "required-validator",
                              "name": "QuantityRequiredValidator",
                              "component": "Quantity",
                              "text": "Quantity is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new OrdersProduct!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "net5wasmconn.createOrdersProduct",
                        "parameters": [
                          {
                            "name": "OrdersProduct",
                            "value": "${ordersproduct}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${ordersproduct}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${ordersproduct}",
                  "visible": "${ordersproduct != null}",
                  "itemType": "Net5Wasm.Models.Net5Wasmconn.OrdersProduct"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "ordersproduct",
        "type": "set",
        "value": "new Net5Wasm.Models.Net5Wasmconn.OrdersProduct(){}",
        "valueType": "Net5Wasm.Models.Net5Wasmconn.OrdersProduct"
      }
    ]
  },
  "name": "Add Orders Product",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}