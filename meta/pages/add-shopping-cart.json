{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Created On",
                              "component": "CreatedOn",
                              "name": "CreatedOnLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "CreatedOn",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${shoppingcart.CreatedOn}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CreatedOnRequiredValidator",
                              "component": "CreatedOn",
                              "text": "CreatedOn is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Modified On",
                              "component": "ModifiedOn",
                              "name": "ModifiedOnLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "ModifiedOn",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${shoppingcart.ModifiedOn}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "User Id",
                              "component": "UserId",
                              "name": "UserIdLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 450,
                              "type": "textbox",
                              "name": "UserId",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${shoppingcart.UserId}"
                            },
                            {
                              "type": "required-validator",
                              "name": "UserIdRequiredValidator",
                              "component": "UserId",
                              "text": "UserId is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new ShoppingCart!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "net5wasmconn.createShoppingCart",
                        "parameters": [
                          {
                            "name": "ShoppingCart",
                            "value": "${shoppingcart}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${shoppingcart}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${shoppingcart}",
                  "visible": "${shoppingcart != null}",
                  "itemType": "Net5Wasm.Models.Net5Wasmconn.ShoppingCart"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "shoppingcart",
        "type": "set",
        "value": "new Net5Wasm.Models.Net5Wasmconn.ShoppingCart(){}",
        "valueType": "Net5Wasm.Models.Net5Wasmconn.ShoppingCart"
      }
    ]
  },
  "name": "Add Shopping Cart",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}